/* pages/heart/heart.wxss */

page {
  height: 100%;
  background-color: #f6f6f6;
}

.tab {
  width: 100%;
  height:160rpx;
  background-color: #fff;
  text-align: center;
  font-size: 26rpx;
  font-weight: bold;
  color: #333;
}

.user-list {
  width: 720rpx;
  margin: 0 auto;
}

.clearfix:after {
  content: " ";
  display: block;
  line-height: 0;
  height: 0;
  visibility: hidden;
  clear: both;
}

.user-title {
  text-align: center;
  color: #999;
  font-size: 26rpx;
  margin: 10rpx 0;
  display: flex;
  justify-content: center;
}

.icon-message {
  width: 50rpx;
  height: 50rpx;
}

.icon-text {
  margin: 0 0 0 10rpx;
  line-height: 50rpx;
}

.userinfo-avatar {
  position: fixed;
  bottom: 200rpx;
  right: 30rpx;
  width: 70rpx;
  height: 70rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}

.userinfo-avatar::after {
  border: none;
}

.user-wrap {
  margin-top:20rpx;
  border-radius: 15rpx;
  background-color: #fff;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
}

.img {
  width: 90rpx;
  height: 90rpx;
  margin: 10rpx;
  border-radius: 50%;
  border: 1px solid #f3f3f3;
}

.text {
  font-size: 24rpx;
  color: #999;
  width: 180rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
