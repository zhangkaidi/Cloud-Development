/* pages/publish/publish.wxss */

/**index.wxss**/

page {
  position: relative;
  height: 100%;
  background-color: #f6f6f6;
}

.user-title {
  text-align: center;
  color: #999;
  font-size: 26rpx;
  margin: 10rpx 0;
  display: flex;
  justify-content: center;
}

.icon-message {
  width: 50rpx;
  height: 50rpx;
}

.icon-text {
  margin: 0 0 0 10rpx;
  line-height: 50rpx;
}

.container {
  position: relative;
  z-index: 1000;
}

.swiper {
  position: fixed;
  top: 0;
  height: 100rpx;
  line-height: 100rpx;
  width: 100%;
  text-align: center;
  font-size: 40rpx;
}

.img {
  float: left;
  width: 100rpx;
  height: 100rpx;
  margin: 10rpx;
  border-radius: 50%;
}

.fl {
  float: left;
}

.fr {
  float: right;
}

.icon {
  width: 70rpx;
  height: 70rpx;
  margin: 10rpx;
  border-radius: 50%;
}

.con {
  position: relative;
  width: 610rpx;
  padding-top: 5rpx;
}

.text {
  font-size: 28rpx;
  margin: 0 5rpx;
}

.text.name {
  font-weight: 500;
  color: #333;
}

.text.time {
  font-size: 24rpx;
  font-weight: 500;
  color: #999;
}

.icon-top {
  position: absolute;
  width: 40rpx;
  height: 22rpx;
  top: 8rpx;
  margin-left: 5rpx;
}

.opration {
  position: absolute;
  top: 8rpx;
  height: 34rpx;
  width: 34rpx;
}

.opration.delete {
  right: 0;
  width: 35rpx;
}

.opration.top {
  right: 60rpx;
}

.tip {
  text-align: center;
  font-size: 24rpx;
  color: #999;
}

.message {
  float: left;
  font-size: 28rpx;
  color: #555;
  width: 610rpx;
  text-align: justify;
  margin-left: 5rpx;
  padding: 10rpx 0;
}

.name {
  position: relative;
  min-width: 270rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.messagelist {
  width: 720rpx;
  margin: 0 auto 80rpx auto;
}

.messagelist .message-wrap {
  margin: 20rpx 0;
  border-radius: 15rpx;
  background-color: #fff;
  padding-top: 8rpx;
}

.messagelist .message-wrap:first-child {
  margin-top: 0;
}

.clearfix:after {
  content: " ";
  display: block;
  line-height: 0;
  height: 0;
  visibility: hidden;
  clear: both;
}

.userinfo {
  position: fixed;
  bottom: 0;
  width: 100%;
}

.right {
  float: right;
  margin-right: 17rpx;
}

.write-text {
  text-indent: 12rpx;
  font-size: 24rpx;
  display: flex;
}

.write-text .input {
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  display: inline-block;
  border: none;
  background-color: #fff;
  box-shadow: 0px 2rpx 18rpx 4rpx rbga(0, 0, 0, 0.1);
  border-radius: 5rpx;
}

.userinfo-avatar {
  position: fixed;
  bottom: 200rpx;
  right: 30rpx;
  width: 80rpx;
  height: 80rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}

.userinfo-avatar::after {
  border: none;
}
