/* pages/publish/publish.wxss */

/**index.wxss**/

page {
  position: relative;
  height: 100%;
}

.swiper {
  position: fixed;
  top: 0;
  height: 100rpx;
  line-height: 100rpx;
  width: 100%;
  text-align: center;
  font-size: 40rpx;
}

.user-title {
  text-align: center;
  color: #999;
  font-size: 30rpx;
  margin: 20rpx 0;
}

.img {
  float: left;
  width: 100rpx;
  height: 100rpx;
  margin: 10rpx;
  border-radius: 50%;
}

.fl {
  float: left;
}

.fr {
  float: right;
}

.icon {
  width: 70rpx;
  height: 70rpx;
  margin: 10rpx;
  border-radius: 50%;
}

.con {
  width: 636rpx;
}

.text {
  font-size: 30rpx;
  color: #333;
  margin: 0 5rpx;
}

.message {
  float: left;
  font-size: 24rpx;
  color: #666;
  width: 636rpx;
  margin-left: 5rpx;
}

.clearfix {
  clear: both;

}
.messagelist {
  height: 820rpx;
  overflow-y: scroll;
}

.user-wrap {
  margin-top: 20rpx;
  max-height: 230rpx;
  overflow-y: scroll;
}

.userinfo {
  position: fixed;
  bottom: 5rpx;
  width: 100%;
}

.right {
  float: right;
  margin-right: 17rpx;
}

.write-text {
  text-indent: 12rpx;
  font-size: 24rpx;
  display: flex;
}

.write-text .input {
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  display: inline-block;
  border: none;
}

.userinfo-avatar {
  position: fixed;
  bottom: 200rpx;
  right: 5rpx;
  width: 140rpx;
  height: 80rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}

.userinfo-avatar::after {
  border: none;
}