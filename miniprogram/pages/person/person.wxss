/* miniprogram/pages/person/person.wxss */

.person {
  width: 100%;
  height: 100%;
}

.person-message button {
  width: 200rpx;
  font-size: 30rpx;
}

.person-message {
  width: 100%;
  height: 450rpx;
  background-image: linear-gradient(160deg, #0078ff 30%, #002abb 80%);
  /* background-image: linear-gradient(160deg, #ff0064 20%,#ca00ff 80%); */
  overflow: hidden;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.tip {
  text-align: center;
  margin-top: 20rpx;
  font-size: 24rpx;
  color: #999;
}

.user-avatar {
  display: block;
  width: 110rpx;
  height: 110rpx;
  border-radius: 50%;
  background-color: #fff;
}

.user-name {
  padding-top: 15rpx;
  color: #fff;
  font-size: 26rpx;
}

.list-wrap {
  width: 720rpx;
  margin: 40rpx auto;
}

.list {
  width: 680rpx;
  margin: 0 auto;
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1px solid #f3f3f3;
  display: flex;
  align-items: center;
}

.icon {
  width: 65rpx;
  height: 55rpx;
}

.icon-right {
  width: 60rpx;
  height: 60rpx;
}

.icon-text {
  display: inline-block;
  width: 600rpx;
  margin-left: 40rpx;
  font-size: 32rpx;
  color: #444;
}

.icon::after {
  border: none;
}

.other {
  margin-top: 270rpx;
  display: flex;
  justify-content: center;
}

.other-list {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.other-list .text {
  margin: 0 40rpx;
  color: #444;
  font-size: 26rpx;
}

.fenxiang .icon {
  background: url('data:image/png;base64,') no-repeat;
  background-size: 55rpx 100%;
}

.kefu .icon {
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}
