/* miniprogram/pages/my/my.wxss */

page {
  width: 100%;
  height: 100%;
}

.slide-image {
  width: 100%;
  height: 100%;
}

.swiper {
  width: 100%;
  height: 100%;
}

.swiper-con {
  position: fixed;
  top: 100rpx;
  left: 0;
  width: 100%;
  padding: 0 20rpx;
  box-sizing: border-box;
  height: 400rpx;
  text-align: center;
}

.item-message {
  width: 100%;
  height: 200rpx;
  color: #fff;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 4;
  overflow: hidden;
}

.userinfo-avatar::after {
  border: none;
}

.userinfo-avatar {
  position: fixed;
  bottom: 200rpx;
  right: 20rpx;
  width: 90rpx;
  height: 90rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}

#myAudio {
  display: none;
}
